<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.tapiwa.swapandtop">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".SwapandTop.Authentication.LoginScreenActivity" />
        <activity android:name=".FrontPage.FrontPageActivity" />
        <activity android:name=".SwapandTop.Authentication.SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".SwapandTop.MainFrontPage.FrontPage.FrontPageActivity" />
        <activity android:name=".SwapandTop.MainFrontPage.Electronics.ElectronicsFrontPageActivity" />
        <activity android:name=".SwapandTop.MainFrontPage.Cars.CarsFrontPageActivity" />
        <activity android:name=".SwapandTop.MainFrontPage.Furniture.FurnitureFrontPageActivity" />
        <activity android:name=".SwapandTop.MainFrontPage.Jewellery.JewelleryFrontPageActivity"></activity>
    </application>

</manifest>